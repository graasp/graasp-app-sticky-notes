{
  "name": "graasp-app-sticky-notes",
  "version": "0.2.0a",
  "private": true,
  "author": "Graasp",
  "contributors": [
    "Juan Carlos Farah",
    "Jérémy La Scala"
  ],
  "homepage": ".",
  "license": "AGPL-3.0-only",
  "dependencies": {
    "@graasp/apps-query-client": "github:graasp/graasp-apps-query-client.git#6/more",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.57",
    "@uppy/core": "1.19.1",
    "@uppy/drag-drop": "1.4.29",
    "@uppy/file-input": "1.5.1",
    "@uppy/react": "1.11.10",
    "@uppy/xhr-upload": "1.7.4",
    "bson-objectid": "2.0.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "i18next": "19.9.0",
    "immutable": "4.0.0",
    "json2csv": "4.5.4",
    "lodash": "4.17.21",
    "prop-types": "15.7.2",
    "qs": "6.10.1",
    "react": "17.0.1",
    "react-csv": "2.0.1",
    "react-dom": "17.0.1",
    "react-ga": "3.3.0",
    "react-i18next": "11.8.8",
    "react-redux": "7.2.2",
    "react-scripts": "4.0.3",
    "react-select": "4.1.0",
    "react-toastify": "7.0.3",
    "react-wordcloud": "1.0.5",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-promise": "0.6.0",
    "redux-thunk": "2.3.0",
    "stopword": "0.2.4"
  },
  "scripts": {
    "pre-commit": "pretty-quick --staged && yarn lint",
    "start": "npm-run-all --parallel start:local",
    "start:local": "env-cmd -f ./.env.local react-scripts start",
    "start:api": "json-server --watch data/db.json --port 3636 --id _id --routes data/routes.json",
    "prestart:api": "cat data/default.json > data/db.json",
    "build": "react-scripts build",
    "build:local": "env-cmd -f ./.env.local yarn build",
    "build:dev": "env-cmd -f ./.env.dev yarn build",
    "build:prod": "env-cmd -f ./.env.prod yarn build",
    "build:version": "REACT_APP_VERSION=$CI_BRANCH env-cmd --no-override -f ./.env.prod yarn build",
    "hooks:uninstall": "node node_modules/husky/husky.js uninstall",
    "hooks:install": "node node_modules/husky/husky.js install",
    "lint": "eslint .",
    "prettier:check": "prettier --check 'src/**/*.js'",
    "prettier:write": "prettier --write 'src/**/*.js'",
    "test": "react-scripts test",
    "test:once": "cross-env CI=true env-cmd -f ./.env.test react-scripts test --env=jsdom",
    "test:coverage": "cross-env CI=true env-cmd -f ./.env.test react-scripts test --env=jsdom --coverage",
    "test:ci": "run-s test:coverage report",
    "report": "cat ./coverage/lcov.info | env-cmd -f ./.env.test codacy-coverage",
    "eject": "react-scripts eject",
    "release": "git fetch --tags && git add CHANGELOG.md && standard-version -a"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.0",
    "@commitlint/config-conventional": "12.0.0",
    "babel-eslint": "10.1.0",
    "codacy-coverage": "3.4.0",
    "cross-env": "7.0.3",
    "cypress": "4.1.0",
    "env-cmd": "10.1.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "glob": "7.1.6",
    "html-webpack-plugin": "3.2.0",
    "husky": "5.1.1",
    "interpolate-html-plugin": "3.0.0",
    "json-server": "0.16.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "script-ext-html-webpack-plugin": "2.1.3",
    "standard-version": "9.1.1",
    "uglifyjs-webpack-plugin": "2.1.3",
    "webpack-cli": "4.5.0"
  },
  "release": {
    "analyzeCommits": "conventional"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "packageManager": "yarn@3.2.0-rc.13"
}
